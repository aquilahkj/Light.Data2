<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Aquila Huang</Authors>
    <Company>Aquila Huang</Company>
    <Description>Mysql provider for Light.Data</Description>
    <Copyright>Copyright Â© 2017</Copyright>
    <PackageLicenseUrl>https://github.com/aquilahkj/Light.Data2/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/aquilahkj/Light.Data2/</PackageProjectUrl>
    <Version>2.0.5</Version>
    <PackageTags>Database, Data, ORM, ADO.NET, SQL, MYSQL</PackageTags>
    <AssemblyVersion>2.0.0.0</AssemblyVersion>
    <FileVersion>2.0.5.0</FileVersion>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MySql.Data" Version="6.10.3-rc" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Light.Data\Light.Data.csproj" />
  </ItemGroup>

</Project>
